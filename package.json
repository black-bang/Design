{
  "name": "ReactWebpack",
  "version": "1.0.0",
  "description": "快速搭建基于webpack的React开发环境",
  "main": "",
  "scripts": {
    "AddJZKER": "git submodule add https://gitee.com/kdzs493/JZKER.git JZKER",
    "start": "git init && npm run AddJZKER",
    "setDev": "set NODE_ENV=development",
    "setProd": "set NODE_ENV=production",
    "removeAssets": "rimraf ./assets",
    "removeTest": "rimraf ./TOOL/TestServer/test",
	 "runTest": "node ./TOOL/TestServer/TestServer.js",
    "webpackDev": "webpack-dev-server --config ./webpack.config/webpack.dev.config.js",
    "webpackTest": "webpack --config ./webpack.config/webpack.test.config.js",
    "webpackAssets": "webpack --config ./webpack.config/webpack.assets.config.js",
    "buildComponent": "webpack --config ./JZKER/JZKER.COMPONENTS/webpack.config/webpack.watch.config.js",
    "dev": "npm run setDev && concurrently \"npm run buildComponent\" \"npm run webpackDev\"",
    "test": "npm run removeTest && npm run setProd && npm run webpackTest && npm run runTest",
    "assets": "npm run removeAssets && npm run setProd && npm run webpackAssets"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "babel": {
    "presets": [
      "./webpack.config/.babelrc.js"
    ]
  },
  "author": "ysc",
  "license": "ISC",
  "devDependencies": {
    "antd-mobile": "^2.1.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-mobx": "^1.0.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "classnames": "^2.2.5",
    "concurrently": "^4.0.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "css-loader": "^0.28.7",
    "d3-format": "^1.3.2",
    "date": "^1.0.2",
    "date-format": "^1.2.0",
    "draft-js": "^0.10.5",
    "es6-promise": "^4.2.2",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "1.1.5",
    "glob": "^7.1.3",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "js-cookie": "^2.2.0",
    "json-loader": "^0.5.7",
    "mobx": "^4.1.1",
